{
  "name": "franklin",
  "version": "0.0.0",
  "description": "Project Franklin Take Home Test",
  "keywords": [
    "franklin",
    "walmart"
  ],
  "main": "./build/lib/server.js",
  "types": "./build/lib/server.d.ts",
  "scripts": {
    "build": "tsc",
    "mocha": "./bin/mocha",
    "clean_compiled": "rm -rf ./build",
    "clean_npm": "rm -rf ./node_modules",
    "compile": "npm run compile_typescript",
    "compile_typescript": "tsc --version && tsc",
    "start": "concurrently \"npm run serve\" \"npm run client\"",
    "client": "node start-client.js",
    "serve": "node build/lib/server.js",
    "rebuild": "npm run clean_compiled && npm run compile",
    "test": "mocha --require ts-node/register src/**/*.spec.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Prashant Bansal",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/request": "^2.48.1",
    "@types/sinon": "^7.0.11",
    "@types/sqlite3": "^3.1.5",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^6.0.2",
    "request": "^2.88.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000"
  },
  "readme": "README.md",
  "engines": {
    "node": "=10.6.0"
  },
  "files": [
    "build/**/*"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "sinon": "^7.3.1",
    "sqlite": "^3.0.3",
    "sqlite3": "^4.0.6",
    "winston": "^3.2.1"
  }
}
